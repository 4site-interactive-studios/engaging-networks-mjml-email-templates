<mjml lang="en">

  <mj-head>
    <mj-title>Amnesty International USA</mj-title>
    <mj-preview>{preview_text}</mj-preview>
    <mj-breakpoint width="600px"/>

    <mj-attributes>
      <mj-all color="#000000" font-size="16px" font-family="Arial, Helvetica, sans-serif"></mj-all>
      <mj-section padding="0px"></mj-section>
      <mj-text padding="0px" css-class="en__component en__component--textblock"></mj-text>
      <mj-image padding="0px" css-class="en__component en__component--textblock"/>
      <mj-divider padding="12px 24px" border-width="2px" border-color="#000000"/>
      <mj-button align="left" padding="0px" inner-padding="10px 20px 10px 20px" border-radius="0px" css-class="en__component en__component--textblock button" background-color="#ffffff" color="#000000" border="2px solid #000000" font-size="18px" font-weight="bold"></mj-button>
      
      <mj-class name="text-header" font-size="32px" line-height="38px" color="#000000" font-weight="bold" text-transform="uppercase"></mj-class>
      <mj-class name="text-body" font-size="16px" line-height="22px" color="#000000"></mj-class>
      <mj-class name="text-signer" font-family="Arial, sans-serif" font-size="16px" line-height="22px" color="#333333"></mj-class>
      <mj-class name="text-footer" font-size="9px" line-height="13px" color="#ffffff"></mj-class>
      <mj-class name="padding-0" padding="0px"></mj-class>
      <mj-class name="padding-top-0" padding-top="0px"></mj-class>
      <mj-class name="padding-bottom-0" padding-bottom="0px"></mj-class>
      <mj-class name="padding-20-0-40-0" padding="20px 0px 40px 0px"></mj-class>
      <mj-class name="padding-20-24-0-24" padding="20px 24px 0px 24px"></mj-class>
      <mj-class name="bold" font-weight="bold"></mj-class>
      <mj-class name="center" align="center"></mj-class>
      <mj-class name="right" align="right"></mj-class>
      <mj-class name="left" align="left"></mj-class>
      <mj-class name="inline-block" display="inline-block"></mj-class>
      <mj-selector path="table">
        <mj-html-attribute name="role">presentation</mj-html-attribute> <!-- Fixes a single table missing this attribute. Located on the email signature image: https://d.pr/i/Vn7ezz -->
      </mj-selector>
    </mj-attributes>

    <mj-style>
      @media only screen and (max-width:599px) {
        /* Breakpoint Styles */
      }
      
      @media (prefers-color-scheme: dark) {

       /* Shows Dark Mode-Only Content, Like Images */
       .dark-img {
          display: block !important;
          width: auto !important;
          overflow: visible !important;
          float: none !important;
          max-height: inherit !important;
          max-width: inherit !important;
          line-height: auto !important;
          margin-top: 0px !important;
          visibility: inherit !important;
        }

        /* Hides Light Mode-Only Content, Like Images */
        .light-img {
          display: none;
          display: none !important;
        }
      }
    </mj-style>

    <mj-style inline="inline">
      .color-cta {
        color: #ffff00;
        text-decoration: underline;
      }

      a.yellow-link {
       text-decoration: underline !important;
       color: #000000 !important;
       background-color: #ff0 !important;
       font-weight: bold !important;
      }

       a.white-link {
       text-decoration: underline !important;
       color: #ffffff !important;
      }

      .underline {
        text-decoration: underline;
      }

      .no-underline {
        text-decoration: none;
      }
      
      .button a {
        display: inline-block;
      }

      .highlight {
        color:#000;
        background-color:#ff0;
        text-decoration:underline;
        font-weight:bold;
      }
    </mj-style>

    <mj-raw>
      <meta name="color-scheme" content="light dark">
      <meta name="supported-color-schemes" content="light dark only">
      <meta name="format-detection" content="telephone=no">
      
      <style>
        img {
          display: block;
          margin-left: auto;
          margin-right: auto;
          font-size: 16px;
          color: #000000;
        }

        /* In the event an editable component is inside another editable component, bump the z-index of its components dialog so it appears above its parent */
        .en__component--active .en__component--active .en__component__actions {
          z-index: 1000;
        }

        .en__component--active .en__component--active .en__component--active .en__component__actions {
          z-index: 1001;
        }

        .en__component--active .en__component--active .en__component--active .en__component--active .en__component__actions {
          z-index: 1002;
        }

        .en__component--active .en__component--active .en__component--active .en__component--active .en__component--active .en__component__actions {
          z-index: 1003;
        }

        /* In the event an editable component is inside another editable component, darken the color its components action button so they appears distinct from its parents */
        .en__component--active .en__component--active .en__component__actions .en__component__action {
          background-color: hsl(191deg 100% 38%);
        }

        .en__component--active .en__component--active .en__component--active .en__component__actions .en__component__action {
          background-color: hsl(191deg 100% 33%);
        }

        .en__component--active .en__component--active .en__component--active .en__component--active .en__component__actions .en__component__action {
          background-color: hsl(191deg 100% 28%);
        }

        .en__component--active .en__component--active .en__component--active .en__component--active .en__component--active .en__component__actions .en__component__action {
          background-color: hsl(191deg 100% 23%);
        }
      </style>

