<mjml lang="en">

  <mj-head>
    <mj-title>World Wildlife Federation</mj-title>

    <mj-preview>World Wildlife Federation</mj-preview>

    <mj-breakpoint width="660px" />

    <mj-attributes>
      <mj-all color="#323232" font-size="16px" font-family="Arial,Helvetica,'Open Sans'"></mj-all>
      <mj-section padding="0px" css-class="en__component en__component--droppable ui-sortable"></mj-section>
      <mj-text padding="12px 24px" css-class="en__component en__component--textblock"></mj-text>
      <mj-image padding="0px" css-class="en__component en__component--imageblock" />
      <mj-divider padding="12px 24px" border-width="2px" border-color="#DEE0E2" />
      <mj-button padding="20px 15px" inner-padding="13px 40px 11px 40px" css-class="en__component en__component--textblock button" font-family="sans-serif" background-color="#d55c19" color="#ffffff" font-size="16px" font-weight="bold" target="_blank" border-radius="23px" border="4px single #ffffff"></mj-button>
      <mj-spacer css-class="en__component en__component--textblock"></mj-spacer>

      <mj-class name="padding-0" padding="0px"></mj-class>
      <mj-class name="padding-top-0" padding-top="0px"></mj-class>
      <mj-class name="padding-bottom-0" padding-bottom="0px"></mj-class>
      <mj-class name="padding-20-0-40-0" padding="20px 0px 40px 0px"></mj-class>
      <mj-class name="padding-20-24-0-24" padding="20px 24px 0px 24px"></mj-class>

      <mj-class name="text-size-small" font-size="12px" line-height="16px"></mj-class>
      <mj-class name="text-size-medium" font-size="18px" line-height="22px"></mj-class>
      <mj-class name="text-size-normal" font-size="16px" line-height="24px"></mj-class>
      <mj-class name="text-size-large" font-size="18px" line-height="27px"></mj-class>
      <mj-class name="text-size-xlarge" font-size="24px" line-height="36px"></mj-class>

      <mj-class name="text-h1-headline" font-family="Arial,Helvetica,'Open Sans'" font-size="44px" line-height="48px" font-weight="700" color="#3b3b3b" padding-left="40px"></mj-class>
      <mj-class name="text-h1-date" font-family="Arial,Helvetica,'Open Sans'" font-size="20px" line-height="20px" font-weight="100" color="#3b3b3b" padding-left="40px"></mj-class>
      <mj-class name="text-h2-headline" font-size="24px" line-height="28px" font-weight="bold" color="#000000"></mj-class>
      <mj-class name="text-h3-subhead" font-size="24px" line-height="30px" font-weight="bold" color="#323232"></mj-class>

      <mj-class name="bold" font-weight="bold"></mj-class>
      <mj-class name="center" align="center"></mj-class>
      <mj-class name="right" align="right"></mj-class>
      <mj-class name="left" align="left"></mj-class>

      <mj-class name="text-color-white" color="#ffffff"></mj-class>

    </mj-attributes>

    <mj-raw>
      <meta name="format-detection" content="telephone=no">
      <style>
        img{
          display: block;
          margin-left: auto;
          margin-right: auto;
        }

        /* In the event an editable component is inside another editable component, bump the z-index of its components dialog so it appears above its parent */
        .en__component--active .en__component--active .en__component__actions {
          z-index: 1000;
        }

        .en__component--active .en__component--active .en__component--active .en__component__actions {
          z-index: 1001;
        }

        .en__component--active .en__component--active .en__component--active .en__component--active .en__component__actions {
          z-index: 1002;
        }

        .en__component--active .en__component--active .en__component--active .en__component--active .en__component--active .en__component__actions {
          z-index: 1003;
        }

        /* In the event an editable component is inside another editable component, darken the color its components action button so they appears distinc from its parents */
        .en__component--active .en__component--active .en__component__actions .en__component__action {
          background-color: hsl(191deg 100% 38%);
        }

        .en__component--active .en__component--active .en__component--active .en__component__actions .en__component__action {
          background-color: hsl(191deg 100% 33%);
        }

        .en__component--active .en__component--active .en__component--active .en__component--active .en__component__actions .en__component__action {
          background-color: hsl(191deg 100% 28%);
        }

        .en__component--active .en__component--active .en__component--active .en__component--active .en__component--active .en__component__actions .en__component__action {
          background-color: hsl(191deg 100% 23%);
        }
      </style>
